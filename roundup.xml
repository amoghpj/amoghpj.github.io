<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{ site.name | xml_escape }}</title>
		<description>{% if site.description %}{{ site.description | xml_escape }}{% endif %}</description>		
		<link></link>
		<atom:link href="/roundup.xml" rel="self" type="application/rss+xml" />
		<title>{{ journalroundup.title | xml_escape }}</title>
		<description>{{ journalroundup.content | xml_escape }}</description>
		<link>{{ journalroundup.url }}</link>
		<guid isPermaLink="true">{{ journalroundup.url }}</guid>
	</channel>
</rss>
